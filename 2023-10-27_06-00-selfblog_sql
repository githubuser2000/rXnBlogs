/*!40000 ALTER TABLE `selfblog_entries` ENABLE KEYS */;
UNLOCK TABLES;

--
-- Table structure for table `selfblog_entrycat`
--

DROP TABLE IF EXISTS `selfblog_entrycat`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `selfblog_entrycat` (
  `entryid` int(11) NOT NULL,
  `categoryid` int(11) NOT NULL,
  UNIQUE KEY `entryid_idx` (`entryid`,`categoryid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci ROW_FORMAT=DYNAMIC;
/*!40101 SET character_set_client = @saved_cs_client */;

--
-- Dumping data for table `selfblog_entrycat`
--

