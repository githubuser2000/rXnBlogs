-- Table structure for table `serendipity_spamblocklog`
--

DROP TABLE IF EXISTS `serendipity_spamblocklog`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `serendipity_spamblocklog` (
  `timestamp` int(10) unsigned DEFAULT NULL,
  `type` varchar(255) DEFAULT NULL,
  `reason` mediumtext DEFAULT NULL,
  `entry_id` int(10) unsigned NOT NULL DEFAULT 0,
  `author` varchar(80) DEFAULT NULL,
  `email` varchar(200) DEFAULT NULL,
  `url` varchar(200) DEFAULT NULL,
  `useragent` varchar(255) DEFAULT NULL,
  `ip` varchar(45) DEFAULT NULL,
  `referer` varchar(255) DEFAULT NULL,
  `body` mediumtext DEFAULT NULL,
  KEY `kspamidx` (`timestamp`),
  KEY `kspamtypeidx` (`type`),
  KEY `kspamentryidx` (`entry_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci ROW_FORMAT=DYNAMIC;
/*!40101 SET character_set_client = @saved_cs_client */;

